{
  "name": "@acter/gateway",
  "description": "Acter API Gateway",
  "version": "0.6.9",
  "main": "src",
  "keywords": [
    "gateway"
  ],
  "author": {
    "name": "Nick Steele",
    "email": "njsteele@gmail.com"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onexdata/reaction-gateway.git"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "nodemon src/",
    "start": "node src/"
  },
  "nodemonConfig": {
    "watch": [
      "src/",
      "public/"
    ],
    "ext": "js,json,html,css,svg",
    "events": {
      "disabled-start": "npm run eslint --fix",
      "disabled-restart": "npm run eslint --fix"
    }
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.16",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^1.2.9",
    "@feathersjs/errors": "^3.3.2",
    "@feathersjs/express": "^1.2.5",
    "@feathersjs/feathers": "^3.2.1",
    "@feathersjs/socketio": "^3.2.7",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "check-disk-space": "^1.5.0",
    "chokidar": "^2.1.2",
    "config": "^2.0.1",
    "debug": "^4.1.1",
    "feathers-memory": "^3.0.2",
    "feathers-rest": "^1.8.1",
    "feathers-sequelize": "^3.1.3",
    "feathers-socketio": "^2.0.1",
    "mysql2": "^1.6.1",
    "os-utils": "0.0.14",
    "portfinder": "^1.0.19",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
